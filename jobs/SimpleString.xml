  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>phpunit --log-junit unit.xml --coverage-clover clover.xml --coverage-html clover tests/*Test.php
</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
phpcs -s -p --report=checkstyle --report-file=&quot;${WORKSPACE}/checkstyle.xml&quot; --standard=Zend --ignore=docs/ ${WORKSPACE}
echo &quot;done phpcs&quot;</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
phpmd . xml codesize,unusedcode,design --reportfile messdetector.xml --exclude tests/
echo &quot;done phpmd&quot;
</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>pdepend --jdepend-xml=jdepend.xml --jdepend-chart=dependencies.svg --overview-pyramid=pyramid.svg --ignore=tests/ .</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
phpcpd --log-pmd phpcpd.xml --exclude tests/ .
echo &quot;done phpcpd&quot;</command>
    </hudson.tasks.Shell>
  </builders>
